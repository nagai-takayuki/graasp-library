{
  "name": "graasp-explore",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "build:prod": "env-cmd -f .env.production razzle build",
    "test": "razzle test --env=jsdom --passWithNoTests",
    "test:once": "cross-env CI=true env-cmd -f ./.env.test razzle test --env=jsdom --passWithNoTests",
    "test:coverage": "cross-env CI=true env-cmd -f ./.env.test razzle test --env=jsdom --coverage --passWithNoTests",
    "start:prod": "NODE_ENV=production node build/server.js",
    "hooks:uninstall": "node node_modules/husky/husky.js uninstall",
    "hooks:install": "node node_modules/husky/husky.js install",
    "lint": "eslint .",
    "prettier:check": "prettier --check 'src/**/*.js'",
    "prettier:write": "prettier --write 'src/**/*.js'",
    "release": "git add CHANGELOG.md && standard-version -a"
  },
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "bson-objectid": "1.3.1",
    "clsx": "1.1.1",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "i18next": "19.8.4",
    "katex": "0.12.0",
    "lodash": "4.17.20",
    "next": "10.0.5",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-ga": "3.3.0",
    "react-i18next": "11.8.5",
    "react-quill": "1.3.5",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "react-toastify": "6.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "babel-plugin-styled-components": "1.12.0",
    "babel-preset-razzle": "3.3.11",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "husky": "4.3.7",
    "mini-css-extract-plugin": "1.3.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "razzle": "3.3.11",
    "razzle-dev-utils": "3.3.11",
    "standard-version": "9.1.0",
    "typescript": "4.1.3",
    "webpack": "5.12.3",
    "webpack-dev-server": "3.11.1"
  }
}
